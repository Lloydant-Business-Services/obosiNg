    <!-- Carousel Script -->

    <script>
        $(document).ready(function () {
            loadVillage();
            $('#show-password').click(function (e) {
                e.preventDefault(); // Prevent the default anchor click behavior
                var passwordInput = $('.password-box input[type="password"]');

                if (passwordInput.attr('type') === 'password') {
                    passwordInput.attr('type', 'text');
                    $(this).html('<i class="fa fa-eye"></i>'); // change icon to 'eye'
                } else {
                    passwordInput.attr('type', 'password');
                    $(this).html('<i class="fa fa-eye-slash"></i>'); // change icon back to 'eye-slash'
                }
            });
        })
        let carouselIndex = 0;
        const carousel = document.querySelector('.carousel');

        function scrollCarousel(direction) {
            let size = GetScreenSize();
            if (size > 795) {
                if (carouselIndex >= 5 && direction == 1)
                    return;
                carouselIndex += direction;
                if (carouselIndex < 0) {
                    carouselIndex = 0;
                } else if (carouselIndex > 7) {
                    carouselIndex = 8;
                }
                carousel.style.transform = `translateX(-${carouselIndex * 260}px)`;
            }
            else {
                if (carouselIndex >= 7 && direction == 1)
                    return;
                carouselIndex += direction;
                if (carouselIndex < 0) {
                    carouselIndex = 0;
                } else if (carouselIndex > 7) {
                    carouselIndex = 8;
                }
                carousel.style.transform = `translateX(-${carouselIndex * 320}px)`;
            }

        }
        function GetScreenSize() {
            return window.screen.width;

        }


        function loadVillage() {


            $.ajax({
                type: 'GET',
                url: '@Url.Action("GetVillages", "Home")', // we are calling json method
                dataType: 'json',
                success: function (result) {
                    console.log(result, "result  akaaaaaa");

                    $("#villageId").empty();
                    //populate ExamBatch
                    $("#villageId").append('<option value = "" > -- Select Village -- </option>');
                    // $.each(result.specification, function (i, specification) {
                    //     $("#akaId").append('<option value = "' + specification.id + '" >' + specification.name + '</option>');
                    // });
                    for (let i = 0; i < result.length; i++) {
                        const item = result[i];
                        // Access each item properties here
                        console.log(item.id, item.unit.name);
                        $("#villageId").append('<option value = "' + item.id + '" >' + item.unit.name + '</option>');
                    }




                },
                error: function (ex) {
                    alert('error' + ex);
                }

            });

        }
        $('#villageId').on('change', function (e) {
            loadAka($('#villageId').val())
        });
        $('#akaId').on('change', function (e) {
            loadUmunna($('#akaId').val())
        });
        $('#umunnaId').on('change', function (e) {
            loadImenne($('#umunnaId').val())
        });

        function loadAka(villageId) {

            if (villageId > 0) {
                $.ajax({
                    type: 'GET',
                    url: '@Url.Action("GetAkas", "Home")', // we are calling json method
                    dataType: 'json',
                    data: { villageId: villageId },
                    success: function (result) {
                        console.log(result, "result  akaaaaaa");

                        $("#akaId").empty();
                        //populate ExamBatch
                        $("#akaId").append('<option value = "" > -- Select Aka -- </option>');
                        // $.each(result.specification, function (i, specification) {
                        //     $("#akaId").append('<option value = "' + specification.id + '" >' + specification.name + '</option>');
                        // });
                        for (let i = 0; i < result.length; i++) {
                            const item = result[i];
                            // Access each item properties here
                            console.log(item.id, item.unit.name);
                            $("#akaId").append('<option value = "' + item.id + '" >' + item.unit.name + '</option>');
                        }




                    },
                    error: function (ex) {
                        alert('error' + ex);
                    }

                });
            }
        }

        function loadUmunna(akaId) {

