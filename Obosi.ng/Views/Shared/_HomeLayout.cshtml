  }
}
.custom-button {
 
 color: #fff ;
 display: block
}
.cancal-button {
  height: 60px;
  border-radius: 4px;
  background-color: #fff;
 color: #fff ;
 display: flex;
}


@@media (max-width: 768px) {
  .custom-button {
      display: block;
  }

  .nav-menu ul {
      flex-direction: column;
      padding-top: 50px; 
  }
}


@@media (min-width: 769px) {
  .custom-button {
      display: none;
  }
}
    .btn-selection-active{
