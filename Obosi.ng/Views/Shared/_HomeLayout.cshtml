<style>
  .navDiv {
position: relative;
margin: 0;
padding: 0;
/* min-height: 100vh;  */
width: 100vw;
overflow: hidden; 
}
.navbar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px 100px;
}

.logo img {
  filter: brightness(100%);
}
@@media (min-width: 768px) {
  .nav-menu ul li::before {
      content: '\2022'; 
      color: red;
      margin-right: 8px;
      font-weight: bold; 
}}
@@media (max-width: 768px) {
.navbar {
    flex-direction: column;
    align-items: center;
    text-align: center;
}

.menu-toggle {
    display: block;
    cursor: pointer;
}

.nav-menu {
    display: none;
    width: 100%;
}

.nav-menu.show-menu {
    display: block;
    position: absolute;
    top: 70px;
    background-color: #04042F;
    z-index: 1;
}

.nav-menu ul {
    flex-direction: column;
}

.nav-menu li {
    margin-right: 0;
    margin-bottom: 10px;
    background-color: #0A0A48;
}

.story {
    padding: 10%;
}

.menu-toggle .bar {
    background-color: #fff; 
}
}

.nav-menu ul {
  list-style: none;
  display: flex;
 
}
.nav-menu li {
  margin-right: 15px;
  font-weight: bold;
  
}
.nav-menu a {
  text-decoration: none;
  color: white;
  position: relative;
}
.nav-item li:active {
    background-color: #fff;
    color: #0078D4;
    border: 1px solid #0078D4;
}

.col.tab:hover{
color: #0078D4;
background-color: #fff;
}
.story {
  color:  #fff;
  font-size: larger;
  align-content: center;
  line-height: "35px";
  padding: 20%;
}
.story p {
  margin: 5px 0; 
}
.story h2 {
  font-size: xx-large;
}
@@media (max-width: 768px) {
  .navbar {
