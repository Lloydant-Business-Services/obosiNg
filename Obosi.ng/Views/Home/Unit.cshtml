        function toggleMenu() {
            var navMenu = document.getElementById('navMenu');
            navMenu.classList.toggle('show-menu');

            var cancelButton = document.querySelector('.custom-button');
            cancelButton.style.display = cancelButton.style.display === 'none' ? 'block' : 'none';
        }

        function closeMenu() {
            var navMenu = document.getElementById('navMenu');
            navMenu.classList.remove('show-menu');

            var cancelButton = document.querySelector('.custom-button');
            cancelButton.style.display = 'none';
        }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@2.11.6/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.min.js"></script>
    <script>
        // JavaScript for tab navigation
        var tabTriggerList = document.querySelectorAll('[data-bs-toggle="pill"]');
        tabTriggerList.forEach(function (tabTrigger) {
            tabTrigger.addEventListener('click', function () {
                var targetTab = document.querySelector(tabTrigger.getAttribute('href'));
                var tabPaneList = document.querySelectorAll('.tab-pane');
                tabPaneList.forEach(function (tabPane) {
                    tabPane.classList.remove('show', 'active');
                });
                targetTab.classList.add('show', 'active');
            });
        });
    </script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            var navLinks = document.querySelectorAll('.nav-item a');

            navLinks.forEach(function (link) {
                link.addEventListener('click', function (event) {
                    // Prevent default link behavior
                    event.preventDefault();

                    navLinks.forEach(function (navLink) {
                        navLink.classList.remove('active-nav-link');
                    });

                    // Add active class to clicked nav link
                    link.classList.add('active-nav-link');
                });
            });
        });
    </script>



