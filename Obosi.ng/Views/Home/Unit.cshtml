        var tabTriggerList = document.querySelectorAll('[data-bs-toggle="pill"]');
        tabTriggerList.forEach(function (tabTrigger) {
            tabTrigger.addEventListener('click', function () {
                var targetTab = document.querySelector(tabTrigger.getAttribute('href'));
                var tabPaneList = document.querySelectorAll('.tab-pane');
                tabPaneList.forEach(function (tabPane) {
                    tabPane.classList.remove('show', 'active');
                });
                targetTab.classList.add('show', 'active');
            });
        });
    </script>

</body>

</html>


